{
    "name": "contaoblackforest/contao-encore-bundle",
    "description": "Contao Encore Bundle",
    "keywords": [
        "contao",
        "bundle",
        "webpack",
        "encore"
    ],
    "type": "contao-bundle",
    "homepage": "https://github.com/contaoblackforest/contao-encore-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Sven Baumann",
            "email": "baumann.sv@gmail.com",
            "homepage": "https://github.com/baumannsven",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "baumann.sv@gmail.com",
        "issues": "https://github.com/contaoblackforest/contao-encore-bundle/issues",
        "wiki": "https://github.com/contaoblackforest/contao-encore-bundle/wiki",
        "irc": "irc://irc.freenode.org/contao.mm",
        "source": "https://github.com/contaoblackforest/contao-encore-bundle"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.13, <5.0",
        "menatwork/contao-multicolumnwizard-bundle": "^3.5",
        "symfony/asset": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-kernel": "^5.4",
        "symfony/webpack-encore-bundle": "^1.16",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "ext-filter": "*"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.12",
        "friendsofsymfony/http-cache": "^2.14",
        "php-http/mock-client": "^1.5",
        "doctrine/dbal": "^3.3",
        "phpcq/all-tasks": "^1.2",
        "psr/container": "^1.1 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "BlackForest\\Contao\\Encore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlackForest\\Contao\\Encore\\Test\\": "tests"
        }
    },
    "extra": {
        "contao-manager-plugin": "BlackForest\\Contao\\Encore\\ContaoManager\\Plugin",
        "branch-alias": {
            "dev-feature/2.0.0": "2.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true
        }
    }
}
